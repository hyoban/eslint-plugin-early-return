{
  "name": "eslint-plugin-hyoban",
  "type": "module",
  "version": "0.2.8",
  "packageManager": "pnpm@9.1.0",
  "description": "Hyoban extended ESLint rules.",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-plugin-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-plugin-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-plugin-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/rule-tester": "^7.8.0",
    "bunchee": "^5.1.5",
    "eslint": "^9.2.0",
    "eslint-config-hyoban": "^2.2.5",
    "eslint-vitest-rule-tester": "^0.2.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
